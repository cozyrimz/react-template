{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Template  for Starting React Websites",
  "main": "build/index.js",
  "scripts": {
    "start": "cross-env REACT_APP_ENV=development parcel index.html",
    "start:fresh": "rm -R .parcel-cache && rm -R dist && cross-env REACT_APP_ENV=development parcel index.html",
    "start:local": "cross-env REACT_APP_ENV=local parcel index.html",
    "start:local:fresh": "rm -R .parcel-cache && rm -R dist && cross-env REACT_APP_ENV=local parcel index.html",
    "build": "cross-env REACT_APP_ENV=production parcel build index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.ts src/**/*.tsx src/**/*.js",
    "format": "prettier --write --check src"
  },
  "keywords": [
    "react",
    "typescript",
    "template",
    "parcel"
  ],
  "author": "Sarim Shah",
  "license": "MIT",
  "dependencies": {
    "@date-io/core": "^2.17.0",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/pickers": "^3.3.11",
    "autosuggest-highlight": "^3.3.4",
    "axios": "^0.21.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "dotenv": "^10.0.0",
    "es6-promise": "^4.2.8",
    "js-cookie": "^3.0.5",
    "material-table": "^1.69.3",
    "material-ui-chip-input": "^1.1.0",
    "parcel": "^2.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.8.0 || ^17.0.0",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^16.8.0 || ^17.0.0",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.16.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.0",
    "react-toast-notifications": "^2.5.1",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-typescript": "^7.22.5",
    "@parcel/packager-raw-url": "2.9.3",
    "@parcel/transformer-image": "2.0.0-rc.0",
    "@parcel/transformer-inline-string": "^2.9.3",
    "@parcel/transformer-sass": "2.0.1",
    "@parcel/transformer-svgo": "^2.0.0-nightly.1739",
    "@parcel/transformer-typescript-tsc": "^2.9.3",
    "@parcel/transformer-webmanifest": "2.0.0-rc.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.3",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^4.3.8",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.41.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": "16.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
